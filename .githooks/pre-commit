#!/bin/bash

# Check where this script is running from
where git
git --version
echo INFO: Pre-commit hook running from $PWD

# Check that the EDAM source file is in the right format (RDF/XML)
#grep -c 'owl:Class' EDAM_dev.owl
sh validate.sh
owlclassfound=$?
# owlclassfound>0 is success
exit $owlclassfound
